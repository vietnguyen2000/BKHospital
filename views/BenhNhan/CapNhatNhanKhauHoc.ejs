<%- include("../partials/header") %> 
<% if(Flag){ %>
    <div class="alert alert-success">
            <strong>Success!</strong>  Bạn đã cập nhập thông tin thành công
          </div>
 <% } %>
 <% if(Error){ %>
    <div class="alert alert-danger">
        <strong>Error!</strong> <%=     Error  %> </div>
<% } %>
<div class="container">
    <main role="main" class="pb-3" style="margin-top: 100px;">
        <h1>Cập nhập thông tin nhân khẩu học</h1>
        <form action="/BenhNhan/CapNhatNhanKhauHoc" method="POST">
			<div class="form-group">
				<label>Họ và tên đệm</label>
					<input type="text"  name="HoVaTenLot" class="form-control">
				<label>Tên</label>
					<input type="text"  name="Ten" class="form-control">
				<label>Số điện thoại</label>
					<input type="text"  name="SDT" class="form-control">
				<label>Ngày sinh</label>
					<input type="date"  name="NgaySinh" class="form-control">
				<label>Dân tộc</label>
					<input type="text"  name="DanToc" class="form-control">
                        
            </div>
			<button class="btn btn-primary w-100">Submit</button>
		</form>
	</main>

<%- include("../partials/footer") %>
<%- include("../partials/ending") %>















